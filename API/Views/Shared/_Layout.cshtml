<!DOCTYPE html>

<html lang="ru">
<head>
    <meta charset="utf-8">
    <meta content="width=device-width" name="viewport" />

    <link href="~/lib/bootstrap/dist/css/docs.css" rel="stylesheet">
    <link href="https://mdbcdn.b-cdn.net/wp-content/themes/mdbootstrap4/css/compiled-4.20.0.min.css?ver=4.20.0" id="compiled.css-css" media="all" rel="stylesheet" type="text/css">

    <link href="~/css/style.css" rel="stylesheet">

    <script>
        function loadcontent(num, url) {
            $.ajax({
                dataType: "html",
                type: "POST",
                url: url,
                data: { id: num },
                success: function (a) {
                    $('#results').html(a);
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    alert(errorThrown);
                }
            });
        };
    </script>

    <link href="https://fonts.googleapis.com" rel="preconnect">
    <link crossorigin href="https://fonts.gstatic.com" rel="preconnect">
    <link href="https://fonts.googleapis.com/css2?family=Roboto&display=swap" rel="stylesheet">
</head>
<body style="background: #bccbe3;">
    <div>
        @RenderBody()
    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" type="text/javascript"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js"></script>
    <script src="https://mdbootstrap.com/api/snippets/static/download/MDB-Pro_4.20.0/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="https://mdbootstrap.com/api/snippets/static/download/MDB-Pro_4.20.0/js/mdb.min.js" type="text/javascript"></script>
    <script src="~/lib/bootstrap/dist/js/docs.min.js"></script>
    <script asp-append-version="true" src="~/js/JavaScript.js"></script>

    @RenderSection("Scripts", required: false)

</body>
</html>