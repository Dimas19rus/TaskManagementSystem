@model HomeViewModel


<div class="modal" id="form-modal" role="dialog" tabindex="-1">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header " >
                <h5 class="modal-title mx-2">Modal title</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <!--<h5 class="modal-title mx-2" ></h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close" ></button>-->
                @*<button type="button" class="close  mx-2" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>*@
            </div>
            <div class="modal-body">

            </div>
        </div>
    </div>
</div>

<div id="view-all">
    <partial model="Model" name="_ViewAll" />
</div>


<script type="text/javascript">
    { // Treeview Initialization
        $(document).ready(function() {
            $('.treeview-animated').mdbTreeview();
        });
    }
</script>


@section Scripts{
    @{ await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
}